{
  "kind": "collectionType",
  "collectionName": "chiens",
  "info": {
    "singularName": "chien",
    "pluralName": "chiens",
    "displayName": "Chiens"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "slug": {
      "type": "uid"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "sex": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "Mâle",
        "Femelle"
      ]
    },
    "birthday": {
      "type": "date",
      "required": true
    },
    "color": {
      "type": "string",
      "default": "brown",
      "required": true
    },
    "retired": {
      "type": "boolean",
      "required": true,
      "default": false
    },
    "banner": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "history": {
      "type": "text"
    },
    "galery": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "health": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "Stérilisé ",
        "Pucé",
        "Vacciné"
      ]
    },
    "hobbies": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "Relaxation",
        "Exploration",
        "Jeux interactifs"
      ]
    },
    "personalities": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "Intelligent",
        "Sensible",
        "Joueur",
        "Loyal"
      ]
    },
    "father_children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chien.chien",
      "mappedBy": "father"
    },
    "father": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::chien.chien",
      "inversedBy": "father_children"
    },
    "mother_children": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::chien.chien",
      "mappedBy": "mother"
    },
    "mother": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::chien.chien",
      "inversedBy": "mother_children"
    }
  }
}
